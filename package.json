{
  "name": "react-backpack",
  "version": "1.0.1",
  "description": "A starter kit for react with SSR, SCSS Modules and more!",
  "main": "index.js",
  "scripts": {
    "dev": "node ./express/development.js",
    "build": "webpack -p --config ./webpack/webpack.production.config.js --progress --profile --colors",
    "prod": "webpack -p --config ./webpack/webpack.production.config.js --progress --profile --colors && node ./express/production.js",
    "pm2": "pm2 start ./express/production.js",
    "clean": "rm -rf node_modules; rm package-lock.json; rm -rf dist; npm i"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "file-loader": "^4.2.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "url-loader": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "css-hot-loader": "^1.4.1",
    "css-loader": "^0.28.4",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "inline-manifest-webpack-plugin": "^4.0.1",
    "isomorphic-style-loader": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.13.0",
    "offline-plugin": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^6.0.5",
    "stats-webpack-plugin": "^0.6.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.41.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
